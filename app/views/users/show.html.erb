<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @user.name %>
</p>

<%= form_for([@user, @user.owned_rooms.build]) do |f| %>
  <div class="field">
    <%= f.label 'Enter room name:' %><br />
    <%= f.text_field :name %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

</br>

<%= link_to 'All rooms', rooms_path %> 
</br>
<%= link_to 'Owned rooms', owned_rooms_user_path(@user) %> 
</br>
<%= link_to 'My rooms', member_rooms_user_path(@user) %> 
</br>
</br>
<%= link_to 'Sign out', signout_path, :method=>:delete %>
